procedure sort(var num:integer); 
var temp,k,loop1,loop2:integer; 
begin 
	for loop1:=1 to num-1 do 
	begin 
		k:= loop1;
		for loop2:=loop1+1 to num do
		if a[k]>a[loop2] then
		k:=loop2; 
		if k <> loop1 then 
		begin 
			temp:=a[k]; 
			a[k]:=a[loop1]; 
			a[loop1]:=temp; 
		end; 
	end; 
end; 
function gcd(m,n:integer):integer; 
begin 
	if n=0 then
		gcd:=m 
	else 
		gcd:=gcd(n,m-(m/n)*n);
end; 
